    "lodash": "^4.7.0",
    "mockdate": "^2.0.1",
    "moment": "^2.14.1",
    "node-libs-browser": "^1.0.0",
    "node-sass": "^3.4.2",
    "pluralize": "^3.0.0",
    "postcss-loader": "^0.8.2",
    "pusher": "^1.5.1",
    "pusher-js": "^4.0.0",
    "react": "^15.2.0",
    "react-addons-css-transition-group": "^15.2.0",
    "react-addons-shallow-compare": "^15.6.2",
    "react-addons-update": "^15.2.1",
    "react-bootstrap": "^0.30.0",
    "react-csv": "^1.0.8",
    "react-datepicker": "^0.40.0",
    "react-dates": "=12.7.0",
    "react-dom": "^15.2.1",
    "react-dropzone": "^4.2.8",
    "react-markdown": "^2.4.6",
    "react-on-rails": "^10.1.3",
    "react-radio-buttons": "^1.2.1",
    "react-redux": "^4.4.1",
    "react-router": "^2.0.1",
    "react-select": "^1.1.0",
    "react-sortable": "^1.2.0",
    "react-table": "^6.7.5",
    "redux": "^3.3.1",
    "redux-thunk": "^2.0.1",
    "request": "^2.81.0",
    "resolve-url-loader": "^1.4.3",
    "sass-loader": "^6.0.6",
    "sass-resources-loader": "^1.3.3",
    "style-loader": "^0.13.1",
    "underscore": "^1.8.3",
    "underscore.string": "^3.3.4",
    "url-loader": "^0.5.7",
    "webpack": "^3.8.1",
    "webpack-manifest-plugin": "^2.0.0-rc.2",
    "webpack-merge": "^4.1.2"
  },
  "devDependencies": {
    "@types/jest": "^22.2.0",
    "awesome-typescript-loader": "^3.5.0",
    "babel-cli": "^6.26.0",
    "babel-jest": "=19.0.0",
    "babel-plugin-react-transform": "^2.0.2",
    "body-parser": "^1.15.0",
    "enzyme": "^2.7.1",
    "enzyme-to-json": "^1.5.0",
    "eslint": "^3.13.1",
    "eslint-config-airbnb": "^14.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^3.0.1",
    "eslint-plugin-react": "^6.9.0",
    "estraverse-fb": "^1.3.1",
    "faye-websocket": "^0.11.1",
    "jest": "=19.0.2",
    "jscs": "^2.11.0",
    "memory-fs": "^0.4.1",
    "mime": "^2.2.0",
    "moments": "0.0.2",
    "qs": "^6.5.1",
    "react-addons-test-utils": "^15.4.2",
    "react-test-renderer": "^15.4.2",
    "react-transform-hmr": "^1.0.4",
    "redux-test-utils": "^0.2.2",
    "ts-jest": "^22.4.1",
    "typescript": "^2.7.2",
    "webpack-dev-server": "^2.9.4"
  },
  "browser": {
    "fs": false
  },
  "scripts": {
    "lint": "npm run eslint --silent",
    "eslint": "eslint --ext .js,.jsx .",
    "jscs": "jscs --verbose .",
    "jscs:fix": "jscs --verbose -x . --silent",
    "test": "npm run build:test && rspec",
    "build:test": "npm run build:client && npm run build:server",
    "build:production:client": "NODE_ENV=production webpack --config webpack.client.rails.build.config.js",
    "build:production:server": "NODE_ENV=production webpack --config webpack.server.rails.build.config.js",
    "build:client": "webpack --config webpack.client.rails.build.config.js",
    "build:dev:client": "webpack -w --config webpack.client.rails.build.config.js",
    "build:dev:server": "webpack -w --config webpack.server.rails.build.config.js",
    "build:server": "webpack --config webpack.server.rails.build.config.js",
    "hot-assets": "NODE_ENV=development babel-node server-rails-hot.js",
    "install-react-on-rails": "npm install 'react-on-rails'",
    "jest": "jest",
    "jest:watch": "jest --watch",
    "jest:coverage": "jest --coverage",
    "jest:silent": "jest --silent",
    "webpack-dev-server": "webpack-dev-server --config webpack.client.rails.hot.config.js"
  },
  "jest": {
    "snapshotSerializers": [
      "<rootDir>/node_modules/enzyme-to-json/serializer"
    ],
    "transform": {
      "^.+\\.jsx?$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.tsx?$": "ts-jest"
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "moduleDirectories": [
      "node_modules",
      "./app"
    ],
    "globals": {
      "window": true
    },
    "coverageDirectory": "<rootDir>/coverage/",
    "collectCoverage": false,
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less|scss)$": "<rootDir>/__mocks__/styleMock.js"
    },
    "collectCoverageFrom": [
      "app/**/*.{js,jsx}"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/test_data/"
    ]
  }
}
