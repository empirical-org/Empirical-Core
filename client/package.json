{
  "name": "Empirical-Core-Client",
  "description": "Client for Empirical Core",
  "repository": {
    "type": "git",
    "url": "https://github.com/empirical-org/Empirical-Core.git"
  },
  "license": "AGPL-3.0",
  "engines": {
    "node": "7.5.0",
    "npm": "3.9.5"
  },
  "main": "js/app.js",
  "dependencies": {
    "@types/react": "^16.0.40",
