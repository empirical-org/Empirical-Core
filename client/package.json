    "babel-runtime": "^6.6.1",
    "bootstrap-loader": "^2.2.0",
    "bootstrap-sass": "^3.3.7",
    "bootstrap-tooltip": "^3.1.1",
    "bowser": "^1.6.0",
    "css-loader": "^0.23.1",
    "es5-shim": "^4.5.7",
    "expose-loader": "^0.7.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^0.8.5",
    "firebase": "^3.7.0",
    "history": "^2.0.1",
    "imports-loader": "^0.6.5",
    "jquery": "^2.2.2",
    "jquery-ujs": "^1.2.1",
    "json-loader": "^0.5.4",
