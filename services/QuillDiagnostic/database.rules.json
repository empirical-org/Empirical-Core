{
  "rules":   {
    ".read": false,
    ".write": false,
    "sessions": {
        ".indexOn": "lessonID"
      },
      "firebase_accessed": {
        ".read": true
      },
    "v2": {
      "classroom_lesson_sessions": {
        ".write": true,
        ".read": "auth.token.staff === true",
        "$classroom_lesson_session_id": {
          ".read": "root.child('v2/classroom_lesson_sessions/'+$classroom_lesson_session_id+'/teacher_ids/'+auth.uid).exists() || data.child('preview').val() === true || root.child('v2/classroom_lesson_sessions/'+$classroom_lesson_session_id+'/student_ids/'+auth.uid).exists() || !data.exists() || !root.child('v2/classroom_lesson_sessions/'+$classroom_lesson_session_id+'/teacher_ids/').exists() || auth.token.staff === true"
        }
      },
      "classroom_lessons": {
        ".read": true,
        ".write": true
      },
      "concept_feedback": {
        ".read": true,
        ".write": true
      },
      "concepts": {
        ".read": true,
        ".write": true
      },
      "datadash": {
        ".read": true,
        ".write": true
      },
      "diagnosticQuestions": {
        ".read": true,
        ".write": "auth.token.staff === true"
      },
      "fillInBlankQuestions": {
        ".read": true,
        ".write": "auth.token.staff === true"
      },
      "flag": {
        ".read": true,
        ".write": true
      },
      "item-levels": {
        ".read": true,
        ".write": true
      },
      "lesson_edition_metadata": {
        ".read": true,
        ".write": true,
        ".indexOn": ["user_id", "lesson_id"]
      },
      "lesson_edition_questions": {
        ".read": true,
        ".write": true
      },
      "lessons": {
        ".read": true,
        ".write": "auth.token.staff === true"
      },
      "lessons_editions": {
        ".read": true,
        ".write": true
      },
      "pathways": {
        ".read": true,
        ".write": true
      },
      "questions": {
        ".read": true,
        ".indexOn": ["prompt"],
        ".write": "auth.token.staff === true"
      },
      "responses": {
        ".read": true,
        ".indexOn": ["questionUID", "gradeIndex", "text", "createdAt"],
        ".write": true
      },
      "reviews": {
        ".read": true,
        ".indexOn": ["value", "activity_id"],
        ".write": true
      },
      "savedSessions": {
        ".read": true,
        ".write": true
      },
      "sentenceFragments": {
        ".read": true,
        ".write": true
      },
      "sessions": {
        ".read": true,
        ".write": true
      },
      "timeouts": {
        ".read": true,
        ".write": true
      }
    }
  }
}
