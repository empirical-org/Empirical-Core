{
  "name": "quillcomprehension",
  "version": "1.0.0",
  "description": "Quill Comprehension React App",
  "main": "index.js",
  "scripts": {
    "lint": "tslint -c tslint.json 'src/**/*.{ts,tsx}' --exclude 'src/tests/*.{ts,tsx}'",
    "lint:fix": "tslint -c tslint.json 'src/**/*.{ts,tsx}' --exclude 'src/tests/*.{ts,tsx}' --fix",
    "start": "EMPIRICAL_BASE_URL=localhost:3000 webpack-dev-server",
    "test": "jest",
    "jest:u": "jest --updateSnapshot",
    "jest": "jest",
    "build:jenkins": "tsc && webpack --optimize-minimize",
    "build": "npm install && tsc && webpack --progress",
    "build:staging": "npm install && webpack --progress --config webpack.staging.config.js",
    "build:prod": "npm install && webpack --progress --config webpack.prod.config.js",
    "clean": "rd /s/q built"
  },
  "keywords": [
    "quill",
    "comprehension",
    "quillcomprehension"
  ],
  "author": "Quill.org",
  "license": "AGPL-3.0",
  "dependencies": {
    "@types/enzyme-adapter-react-16": "^1.0.5",
    "@types/node": "^12.12.14",
    "@types/query-string": "^6.3.0",
    "@types/react": "^16.9.14",
    "@types/react-css-transition-replace": "^2.1.3",
    "@types/react-dom": "^16.9.4",
    "@types/react-redux": "^7.1.5",
    "@types/react-router": "^5.1.3",
    "@types/react-router-config": "^5.0.1",
    "@types/react-router-dom": "^5.1.3",
    "@types/request": "^2.48.3",
    "babel-jest": "^24.9.0",
    "bulma": "^0.8.0",
    "css-hot-loader": "^1.4.4",
    "enzyme-adapter-react-16": "^1.15.1",
    "fs": "0.0.2",
    "net": "^1.0.2",
    "node-sass": "^4.13.0",
    "query-string": "^6.9.0",
    "react": "^16.12.0",
    "react-contenteditable": "^3.3.2",
    "react-css-transition-replace": "^4.0.2",
    "react-dom": "^16.12.0",
    "react-redux": "^7.1.3",
    "react-router": "^5.1.2",
    "react-router-config": "^5.1.1",
    "react-router-dom": "^5.1.2",
    "react-router-redux": "^4.0.8",
    "redux": "^4.0.4",
    "redux-thunk": "^2.3.0",
    "request": "^2.88.0",
    "sass": "^1.23.7",
    "sass-loader": "^8.0.0",
    "style-loader": "^1.0.1",
    "tls": "0.0.1"
  },
  "devDependencies": {
    "@types/enzyme": "^3.10.3",
    "@types/jest": "^24.0.23",
    "compression-webpack-plugin": "^3.0.1",
    "css-loader": "^3.2.1",
    "enzyme": "^3.10.0",
    "enzyme-to-json": "^3.4.3",
    "file-loader": "^5.0.2",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.9.0",
    "json-loader": "^0.5.7",
    "mini-css-extract-plugin": "^0.8.0",
    "redux-devtools-extension": "^2.13.8",
    "source-map-loader": "^0.2.4",
    "ts-import-plugin": "^1.6.1",
    "ts-jest": "^24.2.0",
    "ts-loader": "^6.2.1",
    "tslint": "^5.20.1",
    "tslint-loader": "^3.5.4",
    "typescript": "^3.7.2",
    "url-loader": "^3.0.0",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.9.0",
    "whatwg-fetch": "^3.0.0"
  },
  "jest": {
    "transform": {
      "^.+\\.(js|jsx)?$": "babel-jest",
      "^.+\\.tsx?$": "ts-jest"
    },
    "globals": {
      "ts-jest": {
        "diagnostics": false
      }
    },
    "testRegex": "\\.(test|spec)\\.(ts|tsx|js)$",
    "setupFiles": [
      "<rootDir>/src/setupTests.ts"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ],
    "testURL": "http://localhost:8010"
  }
}
