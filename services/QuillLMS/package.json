  "jest": {
    "roots": [
      "client/app"
    ],
    "snapshotSerializers": [
      "<rootDir>/node_modules/enzyme-to-json/serializer"
    ],
    "transform": {
      "^.+\\.jsx?$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.tsx?$": "ts-jest"
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "moduleDirectories": [
      "node_modules",
      "./client/app"
    ],
    "globals": {
      "window": {},
      "import.meta.env.VITE_PROCESS_ENV_CDN_URL": "http://cdn.quill.org",
      "ts-jest": {
        "diagnostics": false
      }
    },
    "testEnvironment": "jsdom",
    "coverageDirectory": "<rootDir>/client/coverage/",
    "collectCoverage": false,
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/client/__mocks__/fileMock.js",
      "\\.(css|less|scss)$": "<rootDir>/client/__mocks__/styleMock.js"
    },
    "collectCoverageFrom": [
      "app/**/*.{js,jsx}"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/test_data/"
    ],
    "testPathIgnorePatterns": [
      "test_data.js",
      "data.ts"
    ],
    "setupFiles": [
      "<rootDir>/client/setupTests.ts",
      "jest-localstorage-mock"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/client/setupTestFilesAfterEnv.ts"
    ],
    "transformIgnorePatterns": [
      "/node_modules/(?!(jest-runtime)/)"
    ]
  }
