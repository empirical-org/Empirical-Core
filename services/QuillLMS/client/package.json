  "browser": {
    "fs": false
  },
  "scripts": {
    "lint": "npm run eslint --silent",
    "eslint": "eslint --ext .js,.jsx .",
    "jscs": "jscs --verbose .",
    "jscs:fix": "jscs --verbose -x . --silent",
    "test": "npm run build:test && rspec",
    "jest": "node --expose-gc ./node_modules/.bin/jest --maxWorkers=1 --logHeapUsage",
    "jest:watch": "jest --watch",
    "jest:coverage": "jest --coverage",
    "jest:silent": "jest --silent"
  },
  "jest": {
    "roots": [
      "app"
    ],
    "snapshotSerializers": [
      "<rootDir>/node_modules/enzyme-to-json/serializer"
    ],
    "transform": {
      "^.+\\.jsx?$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.tsx?$": "ts-jest"
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "moduleDirectories": [
      "node_modules",
      "./app"
    ],
    "globals": {
      "window": {},
      "import.meta.env.VITE_PROCESS_ENV_CDN_URL": "http://cdn.quill.org",
      "ts-jest": {
        "diagnostics": false
      }
    },
    "testEnvironment": "jsdom",
    "coverageDirectory": "<rootDir>/coverage/",
    "collectCoverage": false,
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less|scss)$": "<rootDir>/__mocks__/styleMock.js"
    },
    "collectCoverageFrom": [
      "app/**/*.{js,jsx}"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/test_data/"
    ],
    "testPathIgnorePatterns": [
      "test_data.js",
      "data.ts"
    ],
    "setupFiles": [
      "<rootDir>/setupTests.ts",
      "<rootDir>/setEnvVars.js",
      "jest-localstorage-mock"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/setupTestFilesAfterEnv.ts"
    ]
  }
